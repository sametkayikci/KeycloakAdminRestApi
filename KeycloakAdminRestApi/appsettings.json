{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Swagger": {
    "Title": "Keycloak Admin REST API Documentation",
    "Version": "v1",
    "IncludeXmlComments": "KeycloakAdminRestApi.xml"
  },
  "KeycloakOptions": {
    "Url": "http://yourkeycloakserver:port",
    "Realm": "yourrealm",
    "ClientId": "yourrealm-client",
    "ClientSecret": "ClientSecret",
    "Authority": "http://yourkeycloakserver:port/realms/inventory",
    "AdminClientId": "admin-cli",
    "AdminSecret": "AdminSecret",
    "PublicKey": "PublicKey"
  }
}
